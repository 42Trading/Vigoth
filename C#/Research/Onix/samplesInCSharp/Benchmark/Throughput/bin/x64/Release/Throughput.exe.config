<?xml version="1.0"?>
<configuration>
	<configSections>
		<section name="FixEngine" type="System.Configuration.NameValueSectionHandler"/>
	</configSections>
	<FixEngine>
		<add key="ListenPort" value="4501"/>
		<add key="Log.InboundMessages" value="false"/>
		<add key="Log.FlushFileAfterEachMessage" value="false"/>
		<add key="Connection.TcpNoDelayOption.SetForAcceptors" value="false"/>
	</FixEngine>
	<appSettings>
		<add key="Number of messages" value="10000"/>
		<add key="Number of sessions" value="1"/>
		<add key="Measure latency" value="True"/>
	</appSettings>
	<startup></startup></configuration>












































